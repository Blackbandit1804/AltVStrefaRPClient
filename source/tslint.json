{
	"defaultSeverity": "error",
	"extends": "tslint-config-airbnb",
	"jsRules": {},
	"rules": {
		"indent": [
			true,
			"tabs"
		],
		"ter-indent": [
			true,
			"tab"
		],
		"semicolon": [
			true,
			"always"
		],
		"max-line-length": [
			true,
			160
		],
		"no-increment-decrement": false
	},
	"rulesDirectory": []
}